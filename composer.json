{
    "name": "zahid/hello-world",
    "type": "library",
    "description": "Laravel is a PHP HTTP client library",
    "keywords": [
        "framework",
        "http",
        "rest",
        "web service",
        "curl",
        "client",
        "HTTP client"
    ],
    "homepage": "https://github.com/zahidcse/",
    "license": "MIT",
    "authors": [
        {
            "name": "zahid Hasan",
            "email": "zahiislink@gmail.com",
            "homepage": "https://github.com/zahidcse"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0"
    },
    "suggest": {
        "psr/log": "Required for using the Log middleware"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        }
    },
    "autoload": {
        "psr-4": {
            "Zahidcse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zahidcse\\Tests\\": "tests/"
        }
    }
}

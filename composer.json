{
    "name": "zahidcse/hello-world",
    "type": "library",
    "description": "Hellow is a PHP HTTP client library",
    "keywords": ["laravel"],
    "homepage": "http://mage-people.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Zahid Hasan",
            "email": "zahiislink@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Zahidcse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zahidcse\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "psr/log": "Required for using the Log middleware"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        }
    }
}
